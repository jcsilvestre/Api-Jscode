# Database Configuration for Docker
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres123
DB_DATABASE=api_jscode
DB_SYNCHRONIZE=false
DB_LOGGING=false

# Application Configuration
NODE_ENV=production
PORT=3000

# JWT Configuration (generate a secure secret in production)
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# CORS Configuration
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=100

# Logging
LOG_LEVEL=info

# Health Check
HEALTH_CHECK_ENABLED=true